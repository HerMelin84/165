import re
import urllib2
import sys
def get_html(link):
    try:
        testfile = urllib2.urlopen(link)
        html = testfile.read()
        return html
    except ValueError:
        return 0


def finn_xml(arg):
    stadnamn=arg
    xml_string=get_html(sys.argv[1])
    url_list=[]
    pattern_stad = 'http.*?/st\wd/.*?/'+arg+'/.*?xml'
    regex = re.search(pattern_stad,xml_string)
    print regex.group(0)
finn_xml(sys.argv[2])
